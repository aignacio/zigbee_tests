    302          static void ParkWay_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    303          {
    304            switch ( inMsg->clusterID )
    305            {
    306              case End_Device_Bind_rsp:
    307                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    308                {
    309                  // Light LED
    310                  //HalLedSet ( HAL_LED_2, HAL_LED_MODE_FLASH );
    311                }
    312                break;
    313          
    314              case Match_Desc_rsp:
    315                {
    316                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    317                  if ( pRsp )
    318                  {
    319                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    320                    {
    321                      ParkWay_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    322                      ParkWay_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    323                      // Take the first endpoint, Can be changed to search through endpoints
    324                      ParkWay_DstAddr.endPoint = pRsp->epList[0];
    325          
    326                      // Light LED
    327                      //HalLedSet( HAL_LED_2, HAL_LED_MODE_ON );
    328          
    329                      //This stop the peer from send request do the coordinator
    330                      //HalLedSet ( HAL_LED_1, HAL_LED_MODE_OFF);
    331                      osal_stop_timerEx( ParkWay_TaskID,POLLING_OF_DEV_EVT);
    332                    }
    333                    osal_mem_free( pRsp );
    334                  }
    335                }
    336                break;
    337            }
    338          }
